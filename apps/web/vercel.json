{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "serverFiles": ["build/server/**"],
  "routes": [
    {
      "src": "/build/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "functions": {
    "build/server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}