    "@rollup/rollup-linux-x64-musl": "npm:rollup@^4.0.0",
    "@rollup/rollup-linux-arm64-gnu": "npm:rollup@^4.0.0",
    "@rollup/rollup-linux-arm64-musl": "npm:rollup@^4.0.0",
